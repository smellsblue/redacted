{
    "name": "Redacted",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "This extension hides the message content of GMail chat notifications.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://*.google.com/*"],
            "all_frames": true,
            "js": ["redacted.js"]
        }
    ],
    "permissions": [
        "tabs",
        "https://*.google.com/*"
    ]
}
